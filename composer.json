{
  "name": "denismitr/laravel-event-recorder",
  "description": "Laravel Event Recorder",
  "keywords": [
    "denismitr",
    "laravel",
    "event-recording",
    "events",
    "event-sourcing"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Denis Mitrofanov",
      "email": "denis.mitr@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "denismitr/laravel-json-attributes": "^0.2",
    "illuminate/console": "^5.6",
    "illuminate/database": "^5.6",
    "illuminate/events": "^5.6",
    "illuminate/support": "^5.6",
    "symfony/property-access": "^4.0",
    "symfony/serializer": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "mockery/mockery": "^1.1",
    "orchestra/testbench": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Denismitr\\EventRecorder\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Denismitr\\EventRecorder\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor\\bin\\phpunit",
    "test-coverage": "vendor\\bin\\phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Denismitr\\EventRecorder\\EventRecorderServiceProvider"
      ]
    }
  }
}
